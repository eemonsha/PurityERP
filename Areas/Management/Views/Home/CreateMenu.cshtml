@model PurityERP.Areas.Management.Models.Menu
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <div class="card card-primary mt-5">
        <div class="card-header">
            <h3 class="card-title">Unit Create</h3>
        </div>
        <!-- /.card-header -->
        <!-- form start -->
        <form asp-action="CreateMenu">
            <div class="card-body">
                <div class="form-group">
                    <label for="exampleInputPassword1">Menu Name</label>
                    <input type="text" class="form-control" id="exampleInputPassword1" placeholder="Menu Name" asp-for="ManuName">
                </div>
            </div>
            <!-- /.card-body -->

            <div class="card-footer">
                <button type="submit" class="btn btn-primary">Submit</button>
            </div>
        </form>
    </div>
</body>
</html>